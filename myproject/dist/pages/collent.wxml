
<view class="mouse"> 
  <view class="background" style="height: {{height}}px; width: {{width}}px">
  <view>
    <view class="yes"/>  
    <view class="center">
      <view class="userinfo cat">
        <image class="userinfo-avatar" src="{{avatarUrl}}" background-size="cover"/>
        <view class="userinfo-nickname">{{nickname}}</view>
      </view>
    </view>  

      <view bindtap="a" class="get cat ">
        <view class="din">
          <image class="photo" src="{{product}}"/>
          <text class="text">我的收藏</text>
        </view>
       <view>
         <text class="des">></text>
       </view>
      </view>

      <view bindtap="b" class="get mouse">
        <view class="din">
          <image class="photo" src="{{tickling}}"/>
          <text class="text">反馈意见</text>
        </view>  
        <view>
          <text class="des">></text>
        </view>
      </view>

      <view bindtap="c" class="get">
        <view class="din">
           <image class="photo" src="{{developer}}"/>
           <text class="text">关于我们</text>
        </view> 
        <view>
          <text>></text>
        </view>
      </view>

    </view>
  </view>
</view>